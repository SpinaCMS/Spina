<h4>Undo stack</h4>
<ol class="undo-entries">
  <% for entry in @undoEntries: %>
    <li><%= entry.description %> <%= JSON.stringify(selectedRange: entry.snapshot.selectedRange, context: entry.context) %></li>
  <% end %>
</ol>

<h4>Redo stack</h4>
<ol class="redo-entries">
  <% for entry in @redoEntries: %>
    <li><%= entry.description %> <%= JSON.stringify(selectedRange: entry.snapshot.selectedRange, context: entry.context) %></li>
  <% end %>
</ol>
